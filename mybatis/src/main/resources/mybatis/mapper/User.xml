<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="study.mybatis.domain.UserMapper">
    <select id="findAll" resultType="study.mybatis.domain.UserDTO">
        SELECT * FROM user
    </select>

    <select id="findById" resultType="study.mybatis.domain.UserDTO">
        SELECT * FROM user
        WHERE user_id = #{userId}
    </select>
</mapper>